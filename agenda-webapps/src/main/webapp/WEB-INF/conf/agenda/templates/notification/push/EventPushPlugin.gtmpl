<% eventTitle = _ctx.escapeHTML(eventTitle); %>
<div>
    <div>
        <%
        String message = null;
        message = _ctx.appRes("Notification.agenda.event.created", eventTitle);
        %>
        <%= message %>
    </div>
    <div>
        <a href="<%= eventURL %>">
            <%= _ctx.appRes("Notification.agenda.event.reply") %>
        </a>
    </div>
</div>